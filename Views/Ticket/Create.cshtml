<form asp-action="Create" asp-controller="Ticket" method="post" enctype="multipart/form-data" class="p-8 space-y-8">
@Html.AntiForgeryToken()

<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Ticket Subject <span class="text-red-500">*</span></label>
<input asp-for="Subject" class="w-full bg-slate-50 dark:bg-slate-900 border-slate-200 dark:border-slate-700 rounded-lg px-4 py-2.5 text-sm focus:ring-primary focus:border-primary @(ViewData.ModelState["Subject"]?.Errors.Count > 0 ? "border-red-500" : "")" placeholder="Short summary of the issue" type="text" />
<span asp-validation-for="Subject" class="text-xs text-red-500"></span>
</div>

<div class="grid grid-cols-1 md:grid-cols-2 gap-8">
<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Problem Type <span class="text-red-500">*</span></label>
<select asp-for="Category" class="w-full bg-slate-50 dark:bg-slate-900 border-slate-200 dark:border-slate-700 rounded-lg px-4 py-2.5 text-sm focus:ring-primary focus:border-primary @(ViewData.ModelState["Category"]?.Errors.Count > 0 ? "border-red-500" : "")">
<option value="">Select a category</option>
<option value="Software Issue">Software Issue</option>
<option value="Hardware Failure">Hardware Failure</option>
<option value="Network/Access Issue">Network/Access Issue</option>
<option value="Email &amp; Communication">Email &amp; Communication</option>
<option value="Asset Request">Asset Request</option>
<option value="Other">Other</option>
</select>
<span asp-validation-for="Category" class="text-xs text-red-500"></span>
</div>
<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Work Location <span class="text-red-500">*</span></label>
<div class="flex items-center gap-6 h-[42px] @(ViewData.ModelState["WorkLocation"]?.Errors.Count > 0 ? "border-2 border-red-500 rounded-lg p-2" : "")">
<label class="flex items-center gap-2 cursor-pointer group">
<input asp-for="WorkLocation" class="size-4 text-primary focus:ring-primary bg-slate-100 dark:bg-slate-900 border-slate-300 dark:border-slate-700" value="Office" type="radio" checked />
<span class="text-sm font-medium text-slate-600 dark:text-slate-400 group-hover:text-white transition-colors">Office</span>
</label>
<label class="flex items-center gap-2 cursor-pointer group">
<input asp-for="WorkLocation" class="size-4 text-primary focus:ring-primary bg-slate-100 dark:bg-slate-900 border-slate-300 dark:border-slate-700" value="Work From Home" type="radio" />
<span class="text-sm font-medium text-slate-600 dark:text-slate-400 group-hover:text-white transition-colors">Work From Home</span>
</label>
</div>
<span asp-validation-for="WorkLocation" class="text-xs text-red-500"></span>
</div>
</div>

<div class="grid grid-cols-1 md:grid-cols-2 gap-8">
<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Priority <span class="text-red-500">*</span></label>
<select asp-for="Priority" class="w-full bg-slate-50 dark:bg-slate-900 border-slate-200 dark:border-slate-700 rounded-lg px-4 py-2.5 text-sm focus:ring-primary focus:border-primary @(ViewData.ModelState["Priority"]?.Errors.Count > 0 ? "border-red-500" : "")">
<option value="Low">Low</option>
<option value="Medium">Medium</option>
<option value="High">High</option>
<option value="Critical">Critical</option>
</select>
<span asp-validation-for="Priority" class="text-xs text-red-500"></span>
</div>
<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Department <span class="text-red-500">*</span></label>
<input asp-for="Department" class="w-full bg-slate-50 dark:bg-slate-900 border-slate-200 dark:border-slate-700 rounded-lg px-4 py-2.5 text-sm focus:ring-primary focus:border-primary @(ViewData.ModelState["Department"]?.Errors.Count > 0 ? "border-red-500" : "")" placeholder="e.g., Finance / HR / IT" type="text" />
<span asp-validation-for="Department" class="text-xs text-red-500"></span>
</div>
</div>

<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Description <span class="text-red-500">*</span></label>
<div class="border border-slate-200 dark:border-slate-700 rounded-lg overflow-hidden">
<div class="bg-slate-50 dark:bg-slate-800/50 border-b border-slate-200 dark:border-slate-700 p-2 flex gap-1">
<button class="p-1.5 hover:bg-slate-200 dark:hover:bg-slate-700 rounded transition-colors" type="button">
<span class="material-symbols-outlined text-[18px]">format_bold</span>
</button>
<button class="p-1.5 hover:bg-slate-200 dark:hover:bg-slate-700 rounded transition-colors" type="button">
<span class="material-symbols-outlined text-[18px]">format_italic</span>
</button>
<button class="p-1.5 hover:bg-slate-200 dark:hover:bg-slate-700 rounded transition-colors" type="button">
<span class="material-symbols-outlined text-[18px]">format_list_bulleted</span>
</button>
<button class="p-1.5 hover:bg-slate-200 dark:hover:bg-slate-700 rounded transition-colors" type="button">
<span class="material-symbols-outlined text-[18px]">link</span>
</button>
<button class="p-1.5 hover:bg-slate-200 dark:hover:bg-slate-700 rounded transition-colors" type="button">
<span class="material-symbols-outlined text-[18px]">image</span>
</button>
</div>
<textarea asp-for="Description" class="w-full bg-slate-50 dark:bg-slate-900 border-none px-4 py-3 text-sm focus:ring-0 min-h-[150px] placeholder:text-slate-500 @(ViewData.ModelState["Description"]?.Errors.Count > 0 ? "border-2 border-red-500" : "")" placeholder="Please describe the issue in detail..."></textarea>
</div>
<span asp-validation-for="Description" class="text-xs text-red-500"></span>
</div>

<div class="grid grid-cols-1 md:grid-cols-2 gap-8">
<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300 flex items-center gap-2">
                                        Team Viewer ID
                                        <span class="material-symbols-outlined text-[16px] text-slate-500 cursor-help" title="Needed for remote troubleshooting">info</span>
                                        <span class="text-red-500">*</span>
</label>
<input name="TeamViewerId" value="" autocomplete="off" class="w-full bg-slate-50 dark:bg-slate-900 border-slate-200 dark:border-slate-700 rounded-lg px-4 py-2.5 text-sm focus:ring-primary focus:border-primary @(ViewData.ModelState["TeamViewerId"]?.Errors.Count > 0 ? "border-red-500" : "")" placeholder="Enter ID" type="text"/>
<span asp-validation-for="TeamViewerId" class="text-xs text-red-500"></span>
</div>
<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Team Viewer Password <span class="text-red-500">*</span></label>
<input name="TeamViewerPassword" value="" autocomplete="off" class="w-full bg-slate-50 dark:bg-slate-900 border-slate-200 dark:border-slate-700 rounded-lg px-4 py-2.5 text-sm focus:ring-primary focus:border-primary @(ViewData.ModelState["TeamViewerPassword"]?.Errors.Count > 0 ? "border-red-500" : "")" placeholder="••••••••" type="password"/>
<span asp-validation-for="TeamViewerPassword" class="text-xs text-red-500"></span>
</div>
</div>

<div class="grid grid-cols-1 md:grid-cols-2 gap-8">
<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Contact Number <span class="text-red-500">*</span></label>
<div class="relative">
<span class="material-symbols-outlined absolute left-3 top-1/2 -translate-y-1/2 text-slate-400 text-[20px]">call</span>
<input asp-for="ContactNumber" class="w-full bg-slate-50 dark:bg-slate-900 border-slate-200 dark:border-slate-700 rounded-lg pl-10 pr-4 py-2.5 text-sm focus:ring-primary focus:border-primary @(ViewData.ModelState["ContactNumber"]?.Errors.Count > 0 ? "border-red-500" : "")" placeholder="+1 (555) 000-0000" type="tel"/>
</div>
<span asp-validation-for="ContactNumber" class="text-xs text-red-500"></span>
</div>
<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Email ID <span class="text-red-500">*</span></label>
<div class="relative">
<span class="material-symbols-outlined absolute left-3 top-1/2 -translate-y-1/2 text-slate-400 text-[20px]">mail</span>
<input asp-for="ContactEmail" class="w-full bg-slate-50 dark:bg-slate-900 border-slate-200 dark:border-slate-700 rounded-lg pl-10 pr-4 py-2.5 text-sm focus:ring-primary focus:border-primary @(ViewData.ModelState["ContactEmail"]?.Errors.Count > 0 ? "border-red-500" : "")" placeholder="alex.morgan@enterprise.com" type="email"/>
</div>
<span asp-validation-for="ContactEmail" class="text-xs text-red-500"></span>
</div>
</div>

<div class="space-y-2">
<label class="text-sm font-bold text-slate-700 dark:text-slate-300">Attachments</label>
<div class="border-2 border-dashed border-slate-200 dark:border-slate-700 rounded-xl p-8 text-center bg-slate-50/30 dark:bg-slate-900/10 hover:border-primary/50 hover:bg-primary/5 transition-all">
<div class="size-12 bg-slate-100 dark:bg-slate-800 rounded-full flex items-center justify-center mx-auto mb-4">
<span class="material-symbols-outlined text-slate-500">upload_file</span>
</div>
<p class="text-sm font-bold">Upload files</p>
<p class="text-xs text-slate-500 mt-1">PNG, JPG, PDF, or LOG files (Max 10MB)</p>
<input asp-for="Attachments" type="file" multiple class="mt-4 block w-full text-sm text-slate-500 file:mr-4 file:py-2 file:px-4 file:rounded-lg file:border-0 file:text-sm file:font-semibold file:bg-slate-100 file:text-slate-700 hover:file:bg-slate-200 dark:file:bg-slate-800 dark:file:text-slate-200" data-val="false" />
</div>
</div>

<div class="flex items-center justify-end gap-4 pt-6 border-t border-slate-100 dark:border-slate-800">
<button type="button" class="px-6 py-2.5 text-sm font-bold text-slate-500 hover:text-slate-700 dark:hover:text-slate-300 transition-colors" onclick="clearForm()">
                                    Discard
                                </button>

<script>
function clearForm() {
    // Clear all form fields
    document.querySelector('form').reset();
    
    // Remove all validation error messages
    document.querySelectorAll('span[data-valmsg-for]').forEach(span => {
        span.textContent = '';
    });
    
    // Remove red borders from all fields
    document.querySelectorAll('input, select, textarea').forEach(field => {
        field.classList.remove('border-red-500', 'border-2', 'border-red-500');
        if (field.tagName === 'TEXTAREA') {
            field.classList.add('border-none');
        } else {
            field.classList.remove('border-2');
        }
    });
    
    // Remove red border from work location container
    const workLocationContainer = document.querySelector('input[name="WorkLocation"]').closest('.space-y-2');
    if (workLocationContainer) {
        workLocationContainer.querySelectorAll('input[type="radio"]').forEach(radio => {
            radio.classList.remove('border-red-500');
            radio.classList.add('border-slate-200', 'dark:border-slate-700');
        });
    }
    
    // Clear validation summary if it exists
    const validationSummary = document.querySelector('.validation-summary-errors');
    if (validationSummary) {
        validationSummary.remove();
    }
}

// Add event listeners to clear validation errors when user starts typing
document.addEventListener('DOMContentLoaded', function() {
    // Clear any client-side validation for attachments on page load
    const attachmentsInput = document.querySelector('input[name="Attachments"]');
    if (attachmentsInput) {
        // Remove any data-val attributes that might trigger validation
        attachmentsInput.removeAttribute('data-val');
        attachmentsInput.removeAttribute('data-val-required');
        attachmentsInput.removeAttribute('data-val-minlength');
        
        // Clear any existing validation messages for attachments
        const attachmentValidation = document.querySelector('span[data-valmsg-for="Attachments"]');
        if (attachmentValidation) {
            attachmentValidation.textContent = '';
            attachmentValidation.className = 'field-validation-valid';
        }
    }
    
    // Add form submit handler to debug and override validation
    const form = document.querySelector('form');
    if (form) {
        form.addEventListener('submit', function(e) {
            console.log('Form submission attempt');
            
            // Check if there are any validation errors
            const validationErrors = document.querySelectorAll('.field-validation-error, .input-validation-error');
            console.log('Validation errors found:', validationErrors.length);
            
            // Force submission by removing any client-side validation blocking
            if (validationErrors.length === 0) {
                console.log('No validation errors, allowing submission');
                return true;
            } else {
                console.log('Validation errors present, checking if they are attachment-related');
                // Allow submission if the only errors are for attachments (which is now optional)
                const nonAttachmentErrors = Array.from(validationErrors).filter(error => {
                    const forAttribute = error.getAttribute('data-valmsg-for');
                    return forAttribute && forAttribute !== 'Attachments';
                });
                
                if (nonAttachmentErrors.length === 0) {
                    console.log('Only attachment validation errors found, allowing submission');
                    // Clear attachment validation errors
                    validationErrors.forEach(error => error.textContent = '');
                    return true;
                }
            }
        });
    }
    
    // Handle input fields and textareas
    document.querySelectorAll('input[type="text"], input[type="email"], input[type="tel"], input[type="password"], textarea').forEach(field => {
        field.addEventListener('input', function() {
            clearFieldValidation(this);
        });
        field.addEventListener('focus', function() {
            clearFieldValidation(this);
        });
    });
    
    // Handle select fields
    document.querySelectorAll('select').forEach(field => {
        field.addEventListener('change', function() {
            clearFieldValidation(this);
        });
    });
    
    // Handle radio buttons (Work Location)
    document.querySelectorAll('input[type="radio"]').forEach(field => {
        field.addEventListener('change', function() {
            const fieldName = this.getAttribute('name');
            clearFieldValidationByName(fieldName);
        });
    });
});

function clearFieldValidation(field) {
    const fieldName = field.getAttribute('asp-for') || field.getAttribute('name');
    if (fieldName) {
        clearFieldValidationByName(fieldName);
    }
}

function clearFieldValidationByName(fieldName) {
    // Clear validation message for this field
    const validationSpan = document.querySelector(`span[data-valmsg-for="${fieldName}"]`);
    if (validationSpan) {
        validationSpan.textContent = '';
    }
    
    // Remove red border from the field
    const field = document.querySelector(`[asp-for="${fieldName}"], [name="${fieldName}"]`);
    if (field) {
        field.classList.remove('border-red-500', 'border-2', 'border-red-500');
    }
    
    // Special handling for Work Location
    if (fieldName === 'WorkLocation') {
        const workLocationContainer = document.querySelector('input[name="WorkLocation"]').closest('.space-y-2');
        if (workLocationContainer) {
            workLocationContainer.querySelectorAll('input[type="radio"]').forEach(radio => {
                radio.classList.remove('border-red-500');
                radio.classList.add('border-slate-200', 'dark:border-slate-700');
            });
        }
    }
}
</script>
